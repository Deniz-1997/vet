{
  "name": "giszp",
  "version": "1.30.6",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --modern",
    "test": "vue-cli-service test:unit --coverage",
    "lint": "run-s lint:*",
    "lint:cc": "eslintcc .",
    "lint:fix": "prettier -w -u .",
    "lint:js": "eslint --fix --color --format node_modules/eslint-friendly-formatter --ignore-path .gitignore .",
    "lint:style": "stylelint src/**/*.{css,scss,vue} --fix",
    "lint:ts": "run-s tsc:*",
    "matrix:parse": "node scripts/bin/matrix/index.js",
    "matrix:prettify": "prettier -w -u ./public/configs/role-model/*.json ./src/models/roles.ts",
    "scripts:build": "tsc -p scripts/tsconfig.json && prettier -w -u ./scripts/bin/**/*.js",
    "tsc:ts": "node scripts/bin/tsc-extended/index.js --type tsc",
    "tsc:vue": "node scripts/bin/tsc-extended/index.js --type vue-tsc",
    "update-matrix": "run-s \"matrix:parse -- {1}\" matrix:prettify --",
    "upload:file": "node scripts/bin/upload/index.js",
    "upload:prettify": "prettier -w -u ./public/configs/files/**/*.json",
    "upload": "run-s \"upload:file -- {1} {2}\" upload:prettify --"
  },
  "dependencies": {
    "@kmdtmyk/vue-fields": "^0.2.5",
    "@types/js-cookie": "^3.0.1",
    "@types/lodash": "^4.14.165",
    "@types/node": "^17.0.15",
    "@types/validatorjs": "^3.15.0",
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-typescript": "^4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "^4.5.0",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "date-fns": "^2.29.3",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "object-hash": "^2.2.0",
    "postcss": "^8.4.5",
    "postcss-html": "^1.3.0",
    "qs": "^6.10.1",
    "sass": "1.32.13",
    "sass-loader": "^10.0.0",
    "typescript": "4.4.2",
    "v-click-outside": "^3.1.2",
    "v-mask": "^2.2.4",
    "validatorjs": "^3.22.1",
    "vue": "^2.6.11",
    "vue-awesome": "^4.1.0",
    "vue-class-component": "^7.2.3",
    "vue-cli-plugin-vuetify": "~2.4.0",
    "vue-good-table": "^2.21.9",
    "vue-imask": "^6.0.7",
    "vue-js-modal": "^2.0.0-rc.6",
    "vue-json-excel": "^0.3.0",
    "vue-meta": "^2.4.0",
    "vue-moment": "^4.1.0",
    "vue-notification": "^1.3.20",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.2.0",
    "vue-select": "^3.11.2",
    "vue-tabs-with-active-line": "^1.2.6",
    "vue-the-mask": "^0.11.1",
    "vue2-datepicker": "^3.9.0",
    "vuedraggable": "^2.24.3",
    "vuejs-datepicker": "^1.6.2",
    "vuejs-paginate": "^2.1.0",
    "vuetify": "^2.4.11",
    "vuetify-loader": "^1.7.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/jest": "^24.0.19",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^1.0.3",
    "app-root-path": "^3.0.0",
    "eslint": "^8.7.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "eslintcc": "^0.7.9",
    "glob": "^7.2.0",
    "husky": "^7.0.4",
    "jest": "^24.2.0",
    "lint-staged": "^12.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "stylelint": "^14.2.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-scss": "^4.1.0",
    "ts-jest": "^24.2.0",
    "vue-eslint-parser": "^8.2.0",
    "vue-template-compiler": "^2.6.11",
    "vue-tsc": "^0.31.1",
    "xlsx": "^0.17.5",
    "yargs": "^17.3.1"
  },
  "lint-staged": {
    "*.{css,scss,vue}": "stylelint --fix",
    "*.{js,ts,vue}": [
      "eslint --fix --color --format node_modules/eslint-friendly-formatter",
      "eslintcc"
    ],
    "*": [
      "prettier -w -u"
    ]
  }
}
